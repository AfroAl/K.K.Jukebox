<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.kkjukebox">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity
            android:name=".ACNLActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".ACCFActivity" />
        <activity android:name=".ACActivity" />

        <service android:name=".SunnyService_acnl" />
        <service android:name=".RainyService_acnl" />
        <service android:name=".SnowyService_acnl" />
        <service android:name=".SunnyService_ac" />
        <service android:name=".RainyService_ac" />
        <service android:name=".kkAlwaysService" />
        <service android:name=".RainyService_accf" />
        <service android:name=".SunnyService_accf" />
        <service android:name=".SnowyService_accf" />
    </application>

</manifest>